.row
  .col.s12
    %h4 Overall Averaged Statistics
.row
  .col.s12
    %table
      %thead
        -@statistics.each do |stat|
          %th
            -if stat.include?('fir') || stat.include?('gir')
              %span=display_regulation_stat(stat)
            -else
              %span=stat.humanize.titleize
